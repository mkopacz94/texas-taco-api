{
  "ApplicationConfiguration": {
    "Cors": {
      "AllowedOrigins": [
        "http://localhost:5173"
      ],
      "AllowedHeaders": [
        "Content-Type",
        "Authorization",
        "X-Requested-With",
        "XSRF-TOKEN"
      ],
      "AllowedMethods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE"
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AuthenticationHttpClient": {
    "BaseAddress": "http://texastaco.authentication.api:8080/api/v1/auth/"
  },
  "AllowedHosts": "*"
}
